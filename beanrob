-- Scripts:

local function QKHGTSA_fake_script() -- Console.LocalScript 
	local script = Instance.new('LocalScript', Console)

	local player = game:GetService("Players").LocalPlayer
	local character = player.Character
	
	local nocliping = nil
	local infjumping = false
	local box = script.Parent.CommandBox
	local faces = {"Front","Back","Bottom","Left","Right","Top"}
	local esp = false
	local loopws = false
	local loopjp = false
	local clip = true
	local stepped
	local function chamobject(tocham, color, transparent) --Cycles through every part in the player's character
		for j, face in pairs(faces) do --Cycles through all 6 faces
			local surface = Instance.new("SurfaceGui") --Insert a new surface gui into the game
			surface.Parent = tocham --The surface gui is placed upon the part we are cycled on
			surface.Name = "Esp" --So we can accsess it later
			surface.Face = Enum.NormalId[face] --Sets the surface gui on the cycled face of the part
			surface.AlwaysOnTop = true --So we can see the gui through walls
			local mainFrame = Instance.new("Frame") --A surface gui is useless if it doesn't display anything
			mainFrame.Parent = surface --Won't see the frame without it being on the surface gui
			mainFrame.BackgroundColor3 = color --Set the color to what we have given it
			mainFrame.BorderSizePixel = 0 --No border issues
			mainFrame.BackgroundTransparency = transparent --Slightly see through it
			mainFrame.Size = UDim2.new(1,0,1,0) --Fill the entire part
		end
	end
	
	game:GetService("UserInputService").InputBegan:Connect(function(input,gameProcessed)
		if gameProcessed == false then -- Prevents this from running when typing in chat
			if input.KeyCode == Enum.KeyCode.Quote then
				script.Parent.Visible = true
				wait(.01)
				box:CaptureFocus()
			end 
		end
	end)
	
	box.FocusLost:Connect(function(enterpress)
		if enterpress then
			script.Parent.Visible = false
			if box.Text:match("bring") or box.Text:match("br") then
				if box.Text:match("all") then
					local targets = {}
	
					for _,v in pairs(workspace.Level.Actors:GetDescendants()) do
						if v:IsA("BasePart") and v.Name == "HumanoidRootPart" then
							if v.Parent.Interact.ObjectName.Value ~= "Rose" and v.Parent.Interact.ObjectName.Value ~= "Manager" and v.Parent.Interact.ObjectName.Value ~= "SC Commander" and v.Parent.Interact.ObjectName.Value ~= "Rivera" and v.Parent.Interact.ObjectName.Value ~= "Jade" and v.Parent.Interact.ObjectName.Value ~= "Ryan" and v.Parent.Interact.ObjectName.Value ~= "Falcon" and v.Parent.Interact.ObjectName.Value ~= "Agent Hemlock" and v.Parent.Interact.ObjectName.Value ~= "Agent Nightshade" and v.Parent.Interact.ObjectName.Value ~= "Auctioneer" then
								table.insert(targets, v.Parent)
							end
						end
					end	
					wait(.1)
					for _,v in pairs(targets) do
						spawn(function()
							v.Humanoid.Jump = true
