										chamobject(v.Character.LowerTorso, Color3.fromRGB(0, 179, 255), 0.2)
									elseif tooltip.Value == "Rose" or tooltip.Value == "Rivera" or tooltip.Value == "Jade" and (not v.Character.LowerTorso:FindFirstChild("Esp")) then
										chamobject(v.Character.LowerTorso, Color3.fromRGB(255, 149, 0), 0.2)
									end
								end
							end
						end
					end
				end
			end)
		end
	end)
end
coroutine.wrap(QKHGTSA_fake_script)()
local function FXHBS_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Visible = false
	end)
end
