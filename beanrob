local player = game:GetService("Players").LocalPlayer
local mouse = player:GetMouse()
mouse.TargetFilter = workspace.Level.Glass
local grabbing = false
local range = nil
local hold = false

game["Run Service"].RenderStepped:connect(
   function()
       setscriptable(game.Players.LocalPlayer, "SimulationRadius", true)
       game.Players.LocalPlayer.SimulationRadius = math.huge, math.huge
   end
)
game.Players.LocalPlayer.SimulationRadiusChanged:Connect(function(radius)
   radius = 9e9
   return radius
end)

local oldtrans = 0
local oldtarget
function change()
         if mouse.Target ~= nil then
            local target = mouse.Target
            if hold == true and target.Transparency == 0 then
                oldtrans = mouse.Target.Transparency
                wait()
                target.Transparency = 1
                oldtarget = target
            elseif hold == false then
                oldtarget.Transparency = oldtrans
            end    
        end   
end    

local UIS = game:GetService("UserInputService")
UIS.InputBegan:Connect(function(input)
    local inputtype = input.UserInputType
    if inputtype == Enum.UserInputType.MouseButton2 then
        hold = true
        change()
    end    
end)

UIS.InputEnded:Connect(function(input)
    local inputtype = input.UserInputType
    if inputtype == Enum.UserInputType.MouseButton2 then
        hold = false
        change()
    end    
end)

game:GetService("UserInputService").InputBegan:Connect(function(Key)
	if Key.KeyCode == Enum.KeyCode.Y then
		print("hai")
		if mouse.Target ~= nil and mouse.Target.Parent.Name ~= "Workspace" and mouse.Target.Parent:FindFirstChildOfClass("Humanoid") ~= nil and grabbing == false then
			targetchar = mouse.Target.Parent
			if targetchar.HumanoidRootPart:FindFirstChild("Hold") then
			    targetchar.HumanoidRootPart.Hold:Destroy()
			end
			if targetchar.HumanoidRootPart:FindFirstChild("Turn") then
			    targetchar.HumanoidRootPart.Turn:Destroy()
			end 
			print(targetchar)
			if targetchar.LowerTorso:FindFirstChild("Esp") then
			    for _,v in pairs(targetchar.LowerTorso:GetDescendants()) do
			        if v.Name == "Esp" then
			            v.Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 255)
			        end 
			    end   
			end    
			grabbing = true
			local bp = Instance.new("BodyPosition")
			bp.P = 10000
			bp.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
			bp.D = 100
			range = (player.Character.HumanoidRootPart.Position - targetchar.HumanoidRootPart.Position).Magnitude
			print(range)
			
			bp.Parent = targetchar.HumanoidRootPart
			stepped = game:GetService("RunService").Heartbeat:Connect(function()
				bp.Position = player.Character.Head.Position + (mouse.Hit.Position - player.Character.Head.Position).Unit * range
			end)
		end
	end
end)

game:GetService("UserInputService").InputBegan:Connect(function(key)
	if key.KeyCode == Enum.KeyCode.Y then
		if grabbing == true and targetchar.HumanoidRootPart.BodyPosition then
			stepped:Disconnect()
			targetchar.HumanoidRootPart.BodyPosition:Destroy()
			if targetchar.LowerTorso:FindFirstChild("Esp") then
			    for _,v in pairs(targetchar.LowerTorso:GetDescendants()) do
			        if v.Name == "Esp" then
			            v:Destroy()
			        end 
			    end   
			end  
			wait(0.5)
			grabbing = false
		end
	end
end)

mouse.Button1Down:Connect(function()
	if grabbing == true and targetchar.HumanoidRootPart.BodyPosition then
		stepped:Disconnect()
		targetchar.HumanoidRootPart.BodyPosition:Destroy()
		if targetchar.LowerTorso:FindFirstChild("Esp") then
			    for _,v in pairs(targetchar.LowerTorso:GetDescendants()) do
			        if v.Name == "Esp" then
			            v:Destroy()
			        end 
			    end   
			end 
		local direction = mouse.Hit.LookVector
		local forceMultipliter = 5000
		targetchar.HumanoidRootPart:ApplyImpulse(direction * forceMultipliter)
		wait(0.5)
		grabbing = false
	end
end)

mouse.WheelForward:Connect(function()
	if range ~= nil then
		range = range + 2.5
	end
end)

mouse.WheelBackward:Connect(function()
	if range ~= nil then
		range = range - 2.5
	end
end)

